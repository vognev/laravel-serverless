{
    "name": "vognev/laravel-serverless",
    "description": "Laravel/Serverless common things",
    "keywords": ["laravel", "serverless"],
    "type": "library",
    "license": "MIT",
    "version": "v0.0.1",
    "authors": [{
        "name": "Vitaliy Ognev",
        "email": "vitaliy.ognev@gmail.com"
    }],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "illuminate/console": "5.8.*",
        "illuminate/contracts": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/http": "5.8.*",
        "laravel/serverless": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Serverless\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Serverless\\ServerlessServiceProvider"
            ]
        }
    }
}
